# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

source 'https://github.com/CocoaPods/Specs.git'

def shared_pods
  pod 'AFNetworking'  
  pod 'Masonry'
  pod 'SDWebImage'
  pod 'IQKeyboardManager'
end

target 'EyeProtectionAction' do
  shared_pods
  
end
